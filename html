  <canvas id="canvas"></canvas>

  <!-- ページのロード時にProcessingスケッチを実行 -->
  <script>
    // キャンバスの要素を取得
    var canvas = document.getElementById("canvas");
    // Processingスケッチをロード
    var processingInstance = new Processing(canvas, "attendance.pde");
  </script>
